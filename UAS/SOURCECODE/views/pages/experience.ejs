<%- contentFor('title')%>
Brisurs Zoo
<%- contentFor('content') %>
<!--Title-->
<div class="container">
    <div class="row my-5">
      <div class="col-md-1 my-1"></div>
      <div class="col-md-10 my-1 text-center">
        <h1 class="text-dark pt-4">Experience Schedule</h1>
        <div class="border-top border-primary w-50 mx-auto my-3"></div>
        <p class="lead">Enjoy Our Activities at Brisurs Zoo</p>
      </div>
    </div>
  </div>
<!--Content-->
<div class="restaurant">
    <div class="container">
      <div class="row my-5 text-center">
        <%for(var i=0; i<expnav.length; i++){ %>
  <!--Image Carousel-->
  <div class="col-md-6 my-1">
      <div id="carousel-<%=i%>" class="carousel slide" data-ride="carousel-<%=i%>" data-interval="6500">
      <!--Carousel Content-->
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img src="/../img/carousel/<%=expnav[i].car1%>" alt="" class="w-100"> <!--w-100 = size 100%-->  
          </div>
          <div class="carousel-item">
            <img src="/../img/carousel/<%=expnav[i].car2%>" alt="" class="w-100"> <!--w-100 = size 100%-->   
          </div>
          <div class="carousel-item">
            <img src="/../img/carousel/<%=expnav[i].car3%>" alt="" class="w-100"> <!--w-100 = size 100%-->   
          </div>
        </div>
      <!--End Carousel Content-->
      <!--Prev n next button-->
      <a href="#carousel-<%=i%>" class="carousel-control-prev" role="button" data-slide="prev">
      <span class="fas fa-chevron-circle-left fa-3x"></span>
      </a>
      <a href="#carousel-<%=i%>" class="carousel-control-next" role="button" data-slide="next">
      <span class="fas fa-chevron-circle-right fa-3x"></span>
      </a>
      <!--Slider bawah-->
      <ol class="carousel-indicators">
        <li data-target="#carousel-<%=i%>" data-slide-to="0" class="active"></li>
        <li data-target="#carousel-<%=i%>" data-slide-to="1"></li>
        <li data-target="#carousel-<%=i%>" data-slide-to="2"></li>
      </ol>
      <!--End button-->
      </div>
        </div>
  <!--End Carousel-->
  <div class="col-md-6 my-1">
      <h1 class="text-dark pt-4 text-center"><%=expnav[i].title%></h1>
        <div class="border-top border-primary w-75 mx-auto my-1"></div>
        <p class="lead text-center"><%=expnav[i].subtitle%></p>
        <p>Time : <%=expnav[i].time1%> - <%=expnav[i].time2%></p>
        <p>Location : <%=expnav[i].location%></p>
        <a href="/experience_detail/<%=i%>" class="btn btn-outline-dark btn-md">Details</a>
  </div>
  
  <div class="col-md-12 my-1">
      <p>&nbsp;</p>
  </div>
  <%}%>
  </div>
    </div>
      </div>
  
  <!--Blank-->
  <div class="col-md-12 my-1">
      <p>&nbsp;</p>
  </div>
  